<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="665.0" prefWidth="360.0"
            xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" styleClass="community.css"
            fx:controller="com.example.webremind.CommunityController">

    <fx:define>
        <Image fx:id="Alarm" url="@../image/alarm.png" />
        <Image fx:id="Home" url="@../image/home.png" />
        <Image fx:id="Mypage" url="@../image/mypage.png" />
        <Image fx:id="Add" url="@../image/add.png" />
    </fx:define>

    <children>
        <HBox alignment="CENTER" layoutY="590.0" prefHeight="66.0" prefWidth="360.0" spacing="50.0">
            <children>
                <Button fx:id="btn_alarm" onAction="#onalarm_comButtonClick">
                    <graphic>
                        <ImageView image="$Alarm"/>
                    </graphic>
                </Button>

                <Button fx:id="btn_home" onAction="#onhome_comButtonClick">
                    <graphic>
                        <ImageView image="$Home"/>
                    </graphic>
                </Button>

                <Button fx:id="btn_mypage" onAction="#onmypage_comButtonClick">
                    <graphic>
                        <ImageView image="$Mypage"/>
                    </graphic>
                </Button>
            </children>
        </HBox>

        <Button fx:id="btn_add" onAction="#onadd_comButtonClick" layoutX="287.0" layoutY="523.0" mnemonicParsing="false">
            <graphic>
                <ImageView image="$Add"/>
            </graphic>
        </Button>

      <VBox layoutY="71.0" prefHeight="445.0" prefWidth="360.0">
         <children>
            <ListView prefHeight="448.0" prefWidth="360.0" />
         </children>
      </VBox>

      <ImageView layoutX="14.0" layoutY="14.0">
         <image>
            <Image url="@../image/Commulogo.png" />
         </image>
      </ImageView>

   </children>
</AnchorPane>
